    <DD_Attribute>
        <name>detector_id</name>
        <version_id>1.0</version_id>
        <local_identifier>detector_id</local_identifier>
        <nillable_flag>false</nillable_flag>
        <submitter_name>Mitchell K. Gordon</submitter_name>
        <definition>An abbreviated name or acronym which identifies the instrument's detector used for this observation. Since WFPC2 generally uses all four of it's detectors, this field is not applicable to WFPC2 observations.
        </definition>
        <DD_Value_Domain>
            <enumeration_flag>true</enumeration_flag>
            <value_data_type>ASCII_Short_String_Collapsed</value_data_type>
<!--- values for ACS -->
       <DD_Permissible_Value>
                <value>HRC</value>
                <value_meaning>High Resolution Channel</value_meaning>
            </DD_Permissible_Value>
       <DD_Permissible_Value>
                <value>SBC</value>
                <value_meaning>Solar Blind Channel</value_meaning>
       </DD_Permissible_Value>
       <DD_Permissible_Value>
                <value>WFC</value>
                <value_meaning>Wide Field Channel</value_meaning>
       </DD_Permissible_Value>
<!--- values for WFC3 -->
       <DD_Permissible_Value>
                <value>UVIS</value>
                <value_meaning>Ultraviolet and Visual channel, sensitive to optical wavelengths from approximately 200 to 1000 nm.</value_meaning>
       </DD_Permissible_Value>
       <DD_Permissible_Value>
                <value>IR</value>
                <value_meaning>Channel sensitive near infrared wavelengths, approximately from 800 to 1700 nm</value_meaning>
       </DD_Permissible_Value>
<!--- values for NICMOS -->
       <DD_Permissible_Value>
                <value>NIC1</value>
                <value_meaning>Highest spatial resolution camera with an 11 x 11 arcsec field of view and 43 milliarcsec sized pixels.</value_meaning>
       </DD_Permissible_Value>
       <DD_Permissible_Value>
                <value>NIC2</value>
                <value_meaning>Intermediate spatial resolution camera with a 19.2 x 19.2 arcsec field of view and 75 milliarcsec pixels.</value_meaning>
       </DD_Permissible_Value>
       <DD_Permissible_Value>
                <value>NIC3</value>
                <value_meaning> lowest spatial resolution with a large 51.2 x 51.2 arcsec field of view and 200 milliarcsec pixels.</value_meaning>
       </DD_Permissible_Value>
<!--- values for STIS -->   
       <DD_Permissible_Value>
                <value>FUV-MAMA</value>
                <value_meaning>One of two MAMA detectors. FUV-MAMA provides coverage from 1150 to 1700 Angstroms</value_meaning>
       </DD_Permissible_Value>
       <DD_Permissible_Value>
                <value>NUV-MAMA</value>
                <value_meaning>One of two MAMA detectors. NUV-MAMA provides coverage from 1600 to 3100 Angstroms</value_meaning>
       </DD_Permissible_Value>
       <DD_Permissible_Value>
                <value>CCD</value>
                <value_meaning>The STIS CCD is a low-noise device capable of high sensitivity in the visible and the NUV.</value_meaning>
       </DD_Permissible_Value>
        </DD_Value_Domain>
    </DD_Attribute>
