<!-- Attributes -->

    <DD_Attribute>
        <name>mast_observation_id</name>
        <version_id>1.0</version_id>
        <local_identifier>mast_observation_id</local_identifier>
        <nillable_flag>false</nillable_flag>
        <submitter_name>Mitchell K. Gordon</submitter_name>
        <definition>An identifier for a particular group of data files in the STScI archive. 
        	In the HST FITS labels the value is given by the keyword ASN_ID, short for association ID. 
        	Certain HST products can only be requested in a group, even though they are different 
        	products as far as we are concerned. This is the 9-character ID that identifies how they 
        	are found in the MAST archive. you can retrieve the associated files from STScI by entering 
        	this ID into the box found on this page: archive.stsci.edu/cgi-bin/dataset_lookup/
        </definition>
        <DD_Value_Domain>
            <enumeration_flag>false</enumeration_flag>
            <value_data_type>ASCII_Short_String_Collapsed</value_data_type>
        </DD_Value_Domain>
    </DD_Attribute>
